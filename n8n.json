{
  "name": "Ads Ingestion + Metrics",
  "nodes": [
    {
      "id": "1",
      "name": "Cron - Cada 60 min",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [
        0,
        0
      ],
      "parameters": {
        "triggerTimes": [
          {
            "mode": "everyX",
            "value": 60,
            "unit": "minutes"
          }
        ]
      }
    },
    {
      "id": "2",
      "name": "Descargar CSV",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        240,
        0
      ],
      "parameters": {
        "url": "https://drive.google.com/uc?export=download&id=1RXj_3txgmyX2Wyt9ZwM7l4axfi5A6EC-",
        "responseFormat": "file",
        "download": true
      }
    },
    {
      "id": "3",
      "name": "Parse CSV",
      "type": "n8n-nodes-base.spreadsheetFile",
      "typeVersion": 1,
      "position": [
        480,
        0
      ],
      "parameters": {
        "operation": "fromBinary",
        "binaryPropertyName": "data"
      }
    },
    {
      "id": "4",
      "name": "A\u00f1adir load_date",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        720,
        0
      ],
      "parameters": {
        "functionCode": "return items.map(item => {\n  item.json.load_date = new Date().toISOString().slice(0, 10);\n  item.json.source_file_name = 'ads_spend.csv';\n  return item;\n});"
      }
    },
    {
      "id": "5",
      "name": "Lotes de 500",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        960,
        0
      ],
      "parameters": {
        "functionCode": "const size = 500;\nconst batches = [];\nfor (let i = 0; i < items.length; i += size) {\n  const chunk = items.slice(i, i + size).map(e => e.json);\n  batches.push({ json: { batch: chunk } });\n}\nreturn batches;"
      }
    },
    {
      "id": "6",
      "name": "POST /ingest",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        1200,
        0
      ],
      "parameters": {
        "url": "https://YOUR-NGROK-SUBDOMAIN.ngrok-free.app/ingest",
        "method": "POST",
        "responseFormat": "json",
        "jsonParameters": true,
        "options": {},
        "bodyParametersJson": "={{$json[\"batch\"]}}",
        "headers": [
          {
            "name": "Content-Type",
            "value": "application/json"
          }
        ]
      }
    },
    {
      "id": "7",
      "name": "Webhook - /metrics",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        480,
        300
      ],
      "parameters": {
        "path": "metrics",
        "httpMethod": "GET",
        "responseMode": "lastNode"
      }
    },
    {
      "id": "8",
